---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
---

<BaseLayout 
  title="Contracts & Codes - Headen Tech Solutions"
  description="Federal contracting information including UEI, DUNS, CAGE codes, NAICS, and PSC codes for Headen Tech Solutions."
>
  <Header />
  
  <main id="main-content">
    <!-- Hero Section -->
    <section class="bg-gradient-to-br from-primary-800 to-primary-900 text-white py-16">
      <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center">
          <h1 class="text-4xl md:text-5xl font-bold mb-6">Contracts & Codes</h1>
          <p class="text-xl text-primary-100 max-w-4xl mx-auto leading-relaxed">
            Federal contracting information and business identifiers for procurement processes.
          </p>
        </div>
      </div>
    </section>

    <!-- Business Information -->
    <section class="py-16">
      <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-semibold text-neutral-900 mb-4">Business Information</h2>
          <p class="text-lg text-neutral-700 max-w-3xl mx-auto">
            Official business identifiers and contracting status.
          </p>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
          <div class="bg-white rounded-2xl p-8 shadow-card border border-neutral-200">
            <h3 class="text-xl font-semibold text-neutral-900 mb-6">Business Status</h3>
            <div class="space-y-4">
              <div class="flex justify-between items-center py-3 border-b border-neutral-200">
                <span class="font-semibold text-neutral-900">Small Business</span>
                <span class="text-primary-600 font-semibold">Yes</span>
              </div>
              <div class="flex justify-between items-center py-3 border-b border-neutral-200">
                <span class="font-semibold text-neutral-900">Business Type</span>
                <span class="text-neutral-700">Technology Services</span>
              </div>
              <div class="flex justify-between items-center py-3 border-b border-neutral-200">
                <span class="font-semibold text-neutral-900">Primary Focus</span>
                <span class="text-neutral-700">Federal Government</span>
              </div>
              <div class="flex justify-between items-center py-3">
                <span class="font-semibold text-neutral-900">Geographic Scope</span>
                <span class="text-neutral-700">Nationwide</span>
              </div>
            </div>
          </div>

          <div class="bg-white rounded-2xl p-8 shadow-card border border-neutral-200">
            <h3 class="text-xl font-semibold text-neutral-900 mb-6">Federal Identifiers</h3>
            <div class="space-y-4">
              <div class="py-3 border-b border-neutral-200">
                <span class="font-semibold text-neutral-900 block mb-1">Unique Entity Identifier (UEI)</span>
                <span class="text-neutral-500 text-sm">TBD - Pending registration</span>
              </div>
              <div class="py-3 border-b border-neutral-200">
                <span class="font-semibold text-neutral-900 block mb-1">DUNS Number</span>
                <span class="text-neutral-500 text-sm">TBD - Pending registration</span>
              </div>
              <div class="py-3 border-b border-neutral-200">
                <span class="font-semibold text-neutral-900 block mb-1">CAGE Code</span>
                <span class="text-neutral-500 text-sm">TBD - Pending registration</span>
              </div>
              <div class="py-3">
                <span class="font-semibold text-neutral-900 block mb-1">SAM Registration</span>
                <span class="text-neutral-500 text-sm">In progress</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- NAICS Codes -->
    <section class="py-16 bg-neutral-100">
      <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-semibold text-neutral-900 mb-4">NAICS Codes</h2>
          <p class="text-lg text-neutral-700 max-w-3xl mx-auto">
            North American Industry Classification System codes for our services.
          </p>
        </div>
        
        <div class="bg-white rounded-2xl p-8 shadow-card border border-neutral-200">
          <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
            <div>
              <h3 class="text-lg font-semibold text-neutral-900 mb-4">Primary NAICS</h3>
              <div class="space-y-3">
                <div class="flex items-center justify-between py-2 px-4 bg-primary-100 rounded-lg">
                  <span class="font-medium text-primary-800">541511</span>
                  <span class="text-sm text-primary-700">Custom Computer Programming Services</span>
                </div>
                <div class="flex items-center justify-between py-2 px-4 bg-primary-100 rounded-lg">
                  <span class="font-medium text-primary-800">541512</span>
                  <span class="text-sm text-primary-700">Computer Systems Design Services</span>
                </div>
              </div>
            </div>
            
            <div>
              <h3 class="text-lg font-semibold text-neutral-900 mb-4">Secondary NAICS</h3>
              <div class="space-y-3">
                <div class="flex items-center justify-between py-2 px-4 bg-neutral-100 rounded-lg">
                  <span class="font-medium text-neutral-800">541519</span>
                  <span class="text-sm text-neutral-700">Other Computer Related Services</span>
                </div>
                <div class="flex items-center justify-between py-2 px-4 bg-neutral-100 rounded-lg">
                  <span class="font-medium text-neutral-800">541690</span>
                  <span class="text-sm text-neutral-700">Other Scientific & Technical Consulting</span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- PSC Codes -->
    <section class="py-16">
      <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-semibold text-neutral-900 mb-4">PSC Codes</h2>
          <p class="text-lg text-neutral-700 max-w-3xl mx-auto">
            Product Service Codes for federal procurement.
          </p>
        </div>
        
        <div class="bg-white rounded-2xl p-8 shadow-card border border-neutral-200">
          <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
            <div>
              <h3 class="text-lg font-semibold text-neutral-900 mb-4">Primary PSC</h3>
              <div class="space-y-3">
                <div class="flex items-center justify-between py-2 px-4 bg-accent-100 rounded-lg">
                  <span class="font-medium text-accent-800">D399</span>
                  <span class="text-sm text-accent-700">IT and Telecom - Other</span>
                </div>
                <div class="flex items-center justify-between py-2 px-4 bg-accent-100 rounded-lg">
                  <span class="font-medium text-accent-800">D399A</span>
                  <span class="text-sm text-accent-700">IT and Telecom - Software</span>
                </div>
              </div>
            </div>
            
            <div>
              <h3 class="text-lg font-semibold text-neutral-900 mb-4">Secondary PSC</h3>
              <div class="space-y-3">
                <div class="flex items-center justify-between py-2 px-4 bg-neutral-100 rounded-lg">
                  <span class="font-medium text-neutral-800">D399B</span>
                  <span class="text-sm text-neutral-700">IT and Telecom - Hardware</span>
                </div>
                <div class="flex items-center justify-between py-2 px-4 bg-neutral-100 rounded-lg">
                  <span class="font-medium text-neutral-800">D399C</span>
                  <span class="text-sm text-neutral-700">IT and Telecom - Services</span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Partnering -->
    <section class="py-16 bg-primary-100">
      <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl font-semibold text-neutral-900 mb-4">Partnering Opportunities</h2>
          <p class="text-lg text-neutral-700 max-w-3xl mx-auto">
            We welcome partnering opportunities as both prime and subcontractor.
          </p>
        </div>
        
        <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
          <div class="bg-white rounded-2xl p-8 shadow-card border border-neutral-200">
            <h3 class="text-xl font-semibold text-neutral-900 mb-4">Prime Contractor</h3>
            <ul class="space-y-3 text-neutral-700">
              <li class="flex items-start">
                <svg class="w-5 h-5 text-primary-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span>AI/ML and data engineering solutions</span>
              </li>
              <li class="flex items-start">
                <svg class="w-5 h-5 text-primary-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span>Geospatial analytics and visualization</span>
              </li>
              <li class="flex items-start">
                <svg class="w-5 h-5 text-primary-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span>Secure software delivery (DevSecOps)</span>
              </li>
            </ul>
          </div>

          <div class="bg-white rounded-2xl p-8 shadow-card border border-neutral-200">
            <h3 class="text-xl font-semibold text-neutral-900 mb-4">Subcontractor</h3>
            <ul class="space-y-3 text-neutral-700">
              <li class="flex items-start">
                <svg class="w-5 h-5 text-primary-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span>Specialized AI/ML expertise</span>
              </li>
              <li class="flex items-start">
                <svg class="w-5 h-5 text-primary-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span>Azure Government implementation</span>
              </li>
              <li class="flex items-start">
                <svg class="w-5 h-5 text-primary-600 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
                </svg>
                <span>Compliance and security consulting</span>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>

    <!-- Contact -->
    <section class="py-16">
      <div class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 text-center">
        <h2 class="text-3xl font-semibold text-neutral-900 mb-6">Interested in Partnering?</h2>
        <p class="text-lg text-neutral-700 mb-8">
          Contact us to discuss prime or subcontractor opportunities.
        </p>
        <a
          href="mailto:consulting@headentechsolutions.com"
          class="inline-flex items-center justify-center bg-primary-600 hover:bg-primary-700 text-white px-8 py-4 rounded-lg text-lg font-semibold transition-colors duration-200 focus:outline-none focus:ring-2 focus:ring-primary-500 focus:ring-offset-2"
        >
          Email Consulting Team
          <svg class="ml-2 w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 4.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"></path>
          </svg>
        </a>
      </div>
    </section>
  </main>
  
  <Footer />
</BaseLayout>
